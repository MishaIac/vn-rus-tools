	SetMessage(UNIT_KASURI, -1, "かすり", "@v04040756「抱かれたい、なんて思ってないから」");
	SetMessage(UNIT_ETC, -1, "", "これはかすりさんからの…告白？");
	SetMessage(UNIT_KASURI, -1, "かすり", "@v04040757「だっておかしいよね？@nもともと嘘の恋人だったんだし」");
	SetMessage(UNIT_ETC, -1, "", "涙混じりの声が、部屋のどこかから聞こえてくる。");
	SetMessage(UNIT_ETC, -1, "", "くそっ、携帯…どこに置いたんだっけ…@n焦れば焦るほど、動きは空回りして…");
	SetMessage(UNIT_KASURI, -1, "かすり", "@v04040758「それじゃ…ね@nわたし、ファミーユ辞めるから…@nみんなにはよろしく伝えてね…」");

