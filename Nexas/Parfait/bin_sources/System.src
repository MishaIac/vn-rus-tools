	// 初回起動チェック
	if(GetSystemFlg(SYSTEMFLG_PROLOGUE) == 0)
	{
		ShowCursor();
		ShowMenu();
		LoadScript("プロローグ.bin", 1);
	}

	SetVisual(VISUAL_GIGA);
