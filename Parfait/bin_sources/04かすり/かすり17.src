	SetMessage(UNIT_KASURI, -1, "かすり", "@v04040754「今から言うこと、全部本当じゃないからね」");
	SetMessage(UNIT_ETC, -1, "", "テーブルの上に置いたはずの携帯を探す。");
	SetMessage(UNIT_KASURI, -1, "かすり", "@v04040755「私は、仁くんのこと、好きじゃないから」");
