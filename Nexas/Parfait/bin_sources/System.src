	// 体験版用
/*
	if(GetEventNo() == EVENT_01_04_01)
	{
		LoadScript("System.bin", 99);
	}
*/
	// クリスマス
	if(GetEventNo() == EVENT_PROLOGUE)
	{
		SetFlg(FLG_XMAS, 1);
		SetFlg(FLG_XMASFLOOR, -1);
		SetFlg(FLG_XMASKITCHEN, -1);
		SetFlg(FLG_XMASFOODCOURT, -1);
		SetFlg(FLG_XMASTOWN, -1);

		// フラグチェック
		if(GetEventFlg(EVENT_04_04_01) != 0)
		{
			SetFlg(FLG_XMASFLOOR, UNIT_ASUKA);
		}

		if( GetEventFlg(EVENT_04_01_01) != 0 && GetEventFlg(EVENT_04_05_04) != 0 && GetEventFlg(EVENT_04_04_04) != 0 )
		{
			SetFlg(FLG_XMASKITCHEN, UNIT_EMA + UNIT_RIKAKO);
		}
		else if(GetEventFlg(EVENT_04_05_04) != 0)
		{
			SetFlg(FLG_XMASKITCHEN, UNIT_EMA);
		}
		else if(GetEventFlg(EVENT_04_04_04) != 0)
		{
			SetFlg(FLG_XMASKITCHEN, UNIT_RIKAKO);
		}
		else if((GetEventFlg(EVENT_02_05_02) != 0 || GetEventFlg(EVENT_03_04_02) != 0) && GetEventFlg(EVENT_03_06_01) != 0 && GetEventFlg(EVENT_04_01_02) != 0)
		{
			SetFlg(FLG_XMASKITCHEN, UNIT_KASURI);
		}
		else
		{
			SetFlg(FLG_XMASKITCHEN, UNIT_HITOSHI);
		}

		if(GetEventFlg(EVENT_04_04_03) != 0 && GetNormalEvent(UNIT_YUI) >= 3)
		{
			SetFlg(FLG_XMASFOODCOURT, UNIT_YUI);
		}
		if(GetEventFlg(EVENT_04_06_03) != 0)
		{
			SetFlg(FLG_XMASTOWN, UNIT_REA);
		}

		BGMPlay(BGM_JINGLE);
		SetDay(12, 24);
		DayEffect();

		SetVisual(VISUAL_SELECTMAP);
		FadeIn();
		ShowMenu();
		ShowDay();
		SelectMap();
		BGMFadeOut();
		FadeOut();

		SetFlg(FLG_XMAS, 0);

		if(GetFlg(FLG_RET) == UNIT_YUI)
		{
			LoadEvent(EVENT_YUI_12);
		}
		else if(GetFlg(FLG_RET) == UNIT_REA)
		{
			LoadEvent(EVENT_REA_12);
		}
		else if(GetFlg(FLG_RET) == UNIT_ASUKA)
		{
			LoadEvent(EVENT_ASUKA_12);
		}
		else if(GetFlg(FLG_RET) == UNIT_KASURI)
		{
			LoadEvent(EVENT_KASURI_10);
		}
		else
		{
			LoadEvent(EVENT_EMA_RIKAKO_12);
		}
	}
	else
	{
		// 第1,第2クォータープロローグ終了時は日付を表示しない。
		if(GetEventNo() != EVENT_01_01_01 && GetEventNo() != EVENT_02_01_01)
		{
			DayEffect();
		}

		SetVisual(VISUAL_SELECTMAP);
		FadeIn();
		BGMPlay(BGM_NICHIJOU3);

		ShowMenu();
		ShowDay();
		SelectMap();
		BGMFadeOut();
		FadeOut();

		Wait(100);

		LoadEvent(GetEventNo());
	}
end();

